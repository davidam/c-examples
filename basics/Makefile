
punteros: 
	gcc punteros.c -o punteros

puntero: 
	gcc puntero.c -o puntero 

celsius: 
	gcc celsius.c -o celsius

printid: 
	gcc printid.c -o printid

parentchild:
	gcc parentchild.c -o parentchild

parentchild2:
	gcc parentchild2.c -o parentchild2

chain-of-pointers:
	gcc chain-of-pointers.c -o chain-of-pointers

waitpid-example:
	gcc waitpid-example.c -o waitpid-example

calendar:
	gcc calendar.c -o calendar

pi:
	g++ pi.cc -o pi

arguments:
	gcc arguments.c -o arguments

arguments2:
	gcc arguments2.c -o arguments2

stderr-example:
	gcc stderr-example.c -o stderr-example

stderr-example2:
	gcc stderr-example2.c -o stderr-example2

mem:
	gcc mem.c -o mem

all: punteros puntero celsius printid parentchild parentchild2 chain-of-pointers waitpid-example calendar pi arguments arguments2 stderr-example stderr-example2 mem

clean:
	rm -f punteros puntero celsius printid parentchild parentchild2	chain-of-pointers waitpid-example calendar pi arguments arguments2 stderr-example stderr-example2 stringcompare mem

